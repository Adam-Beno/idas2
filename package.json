{
  "name": "idas2",
  "version": "1.0.0",
  "description": "School project - Library",
  "main": "index.js",
  "author": "Adam Beno <ada.beno96@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production pm2 start -f lib/server && pm2 logs",
    "build": "rimraf lib dist && babel src -d lib --ignore .test.js --ignore src/client && cross-env NODE_ENV=production webpack -p --progress",
    "start:dev": "cross-env NODE_ENV=development electron -r babel-register .",
    "server:dev": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
    "prebuild": "node_modules/.bin/electron-rebuild -f -w oracledb"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.26.0",
    "bcrypt-nodejs": "^0.0.3",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "casual": "^1.5.14",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "electron": "^1.7.9",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "glob": "^7.1.2",
    "graphiql": "^0.11.2",
    "graphql": "^0.10.4",
    "graphql-custom-types": "^1.1.0",
    "graphql-errors": "^2.1.0",
    "graphql-relay": "^0.5.2",
    "graphql-scalars": "^0.1.3",
    "graphql-server-hapi": "^1.0.0",
    "graphql-tools": "^1.1.0",
    "graphql-type-json": "^0.1.4",
    "handlebars": "^4.0.10",
    "hapi": "16.4.3",
    "hapi-auth-jwt2": "^7.3.0",
    "history": "^4.6.3",
    "html-minifier": "^3.5.2",
    "immutable": "4.0.0-rc.2",
    "inert": "^4.2.0",
    "inline-css": "^2.2.2",
    "isomorphic-fetch": "^2.2.1",
    "join-monster": "^2.0.2",
    "jsonwebtoken": "^7.4.1",
    "jss": "^8.1.0",
    "jss-preset-default": "^3.0.0",
    "jwt-decode": "^2.2.0",
    "knex": "^0.13.0",
    "lodash": "^4.17.4",
    "material-ui": "^1.0.0-beta.16",
    "material-ui-icons": "^1.0.0-beta.15",
    "normalize-css": "^2.3.1",
    "oracledb": "^1.13.1",
    "prop-types": "^15.5.10",
    "query-string": "^5.0.0",
    "react": "^15.6.1",
    "react-apollo": "^1.4.8",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-hot-loader": "next",
    "react-infinite": "^0.12.1",
    "react-intl": "^2.3.0",
    "react-jss": "^7.0.2",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "next",
    "redis": "^2.7.1",
    "redux": "^3.7.2",
    "redux-form": "^7.0.2",
    "redux-immutable": "^4.0.0",
    "redux-persist": "^4.8.3",
    "redux-persist-immutable": "^4.3.0",
    "redux-saga": "^0.15.4",
    "reselect": "^3.0.1",
    "uuid": "^3.1.0",
    "validator": "^8.0.0",
    "validator-js": "^0.2.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-flow-react-proptypes": "^3.4.2",
    "babel-plugin-import": "^1.2.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.5.0",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "electron-rebuild": "^1.6.0",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-lodash": "^2.4.4",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-redux-saga": "^0.4.0",
    "eslint-plugin-security": "^1.3.0",
    "eslint-plugin-xss": "^0.1.8",
    "extract-text-webpack-plugin": "^3.0.0",
    "fetch-mock": "^5.12.1",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.50.0",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "node-pg-migrate": "^2.4.0",
    "nodemon": "^1.11.0",
    "pm2": "^2.6.1",
    "redux-mock-store": "^1.2.3",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1"
  },
  "babel": {
    "presets": [
      "latest",
      "react",
      "es2015",
      "stage-0",
      "flow"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ]
        }
      ],
      "transform-decorators-legacy",
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parser": "babel-eslint",
    "extends": [
      "airbnb",
      "plugin:lodash/recommended"
    ],
    "plugins": [
      "lodash",
      "xss",
      "security",
      "redux-saga",
      "jsx-a11y",
      "react"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "settings": {
      "import/resolver": {
        "babel-module": {}
      }
    },
    "rules": {
      "jsx-a11y/href-no-hash": "off",
      "react/forbid-prop-types": 0,
      "import/prefer-default-export": 0,
      "no-underscore-dangle": 0,
      "max-len": [
        1,
        200
      ]
    }
  }
}
